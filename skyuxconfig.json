{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "mode": "easy",
  "compileMode": "aot",
  "name": "skyux-docs-tools",
  "declareComponentsAutomatically": false,
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "app": {
    "styles": [
      "src/app/public/scss/global-tmp.scss"
    ]
  },
  "testSettings": {
    "unit": {
      "browserSet": "paranoid"
    }
  }
}
